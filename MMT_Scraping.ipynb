    "def scrap_curr_hotel_listing(url,dc):\n",
    "    \n",
    "    try:\n",
    "        req = requests.get(url,headers=headers)\n",
    "    except:\n",
    "        print('error : scrap_curr_hotel_listing')\n",
    "        return []\n",
    "    parsed = BeautifulSoup(req.text,'html.parser')\n",
    "    curr_city_hotels_a = parsed.select('.listingRowOuter a')\n",
    "    curr_city_hotels_prices = parsed.select('.listingRowOuter a #hlistpg_hotel_shown_price')\n",
    "    data = []\n",
    "    for url,price in zip(curr_city_hotels_a,curr_city_hotels_prices):\n",
    "        curl = url['href']\n",
    "        curl = \"https:\"+curl\n",
    "        dc[\"price\"] = price\n",
    "        dc[\"hotelId\"] = str(re.findall(\"hotelId=[0-9]*\",curl)[0][8:])\n",
    "        temp = scrap_curr_hotel_page_filter(curl,dc)\n",
    "        if not(\"failed\" in temp):\n",
    "            data.append(temp)\n",
    "        \n",
    "    return data"
   ]
