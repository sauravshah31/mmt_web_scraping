    "def generate_curr_listing_urls(url,data):\n",
    "    \n",
    "    url1 = url #sort by popularity\n",
    "    url2 = url + \"&sort=reviewRating-asc\" #sort by rating-ascending\n",
    "    url3 = url + \"&sort=reviewRating-desc\" #sort by rating-descending\n",
    "    url4 = url + \"&sort=price-asc\" #sort by price-ascending\n",
    "    url5 = url + \"&sort=price-desc\" #sort by price-descending\n",
    "\n",
    "    \n",
    "    dc = []\n",
    "    s1 = scrap_curr_hotel_listing(url1,data)\n",
    "    s2 = scrap_curr_hotel_listing(url2,data)\n",
    "    s3 = scrap_curr_hotel_listing(url3,data)\n",
    "    s4 = scrap_curr_hotel_listing(url4,data)\n",
    "    s5 = scrap_curr_hotel_listing(url5,data)\n",
    "\n",
    "    dc.extend(s1)\n",
    "    dc.extend(s2)\n",
    "    dc.extend(s3)\n",
    "    dc.extend(s4)\n",
    "    dc.extend(s5)\n",
    "    return dc"
   ]
