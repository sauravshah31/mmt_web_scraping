    "def scrap_curr_hotel_page_filter(url,data,proxy=\"\"):\n",
    "    #we have a lot of url, so we might need proxy (sometimes server restricts bandwidth and no of requests(to prevent DDoS,excess use of resources,... )) \n",
    "    #It seems we don't require proxy\n",
    "    currHotelName = \"\"\n",
    "    currCountry = \"\"\n",
    "    currCity = \"\"\n",
    "    currStarRating = \"\"\n",
    "    currPricePerNight = \"\"\n",
    "    currAmenities = \"\"\n",
    "    currHotelDescription = \"\"\n",
    "    currReviews = \"\"\n",
    "    global post_header\n",
    "    try:\n",
    "        post_header['Referer']=url\n",
    "        post_content['cityCode']=data['city']\n",
    "        post_content['locationId']=data['locusId']\n",
    "        post_content['countryCode'] = data['country']\n",
    "        post_content['hotelId'] = data['hotelId']\n",
    "        global hotelNames\n",
    "        if (data['hotelId'] in hotelNames):\n",
    "            return {\"failed\":True}\n",
    "        \n",
    "        hotelNames.add(data['hotelId'])\n",
    "        if(proxy == \"\"):\n",
    "            req = requests.post(\"https://mapi.makemytrip.com/clientbackend/entity/api/mob/staticDetail?srcClient=DESKTOP&countryCode=IN&language=eng&region=in&currency=INR\",headers=post_header,json=post_content)\n",
    "        else:\n",
    "            req = requests.post(\"https://mapi.makemytrip.com/clientbackend/entity/api/mob/staticDetail?srcClient=DESKTOP&countryCode=IN&language=eng&region=in&currency=INR\",headers=post_header,json=post_content,proxies=proxy)\n",
    "        json_data = json.loads(req.text)\n",
    "        currHotelName = json_data['hotelResult']['name']\n",
    "        currCountry = data['country']\n",
    "        currCity = data['city']\n",
    "        try:\n",
    "            currStarRating = str(json_data['flyfishSummaryResponse']['summary']['TA']['cumulativeRating'])\n",
    "        except:\n",
    "            try:\n",
    "                currStarRating = str(json_data['flyfishSummaryResponse']['summary']['MMT']['cumulativeRating'])\n",
    "            except:\n",
    "                try:\n",
    "                    currStarRating = str(json_data['flyfishSummaryResponse']['summary']['MANUAL']['cumulativeRating'])\n",
    "                except:\n",
    "                    currStarRating = \"\"\n",
    "                \n",
    "        currPricePerNight = data[\"price\"]\n",
    "        currAmenities = json_data['hotelResult']['highlightedAmenities']\n",
    "        currHotelDescription = json_data['hotelResult']['longDescription']\n",
    "        try:\n",
    "            currReviews = [json_data['flyfishSummaryResponse']['summary']['TA']['best'][0]['reviewText'],json_data['flyfishSummaryResponse']['summary']['MMT']['best'][-1]['reviewText']] #first and last best review\n",
    "        except:\n",
    "            try:\n",
    "                currReviews = [json_data['flyfishSummaryResponse']['summary']['MMT']['best'][0]['reviewText'],json_data['flyfishSummaryResponse']['summary']['MMT']['best'][-1]['reviewText']]\n",
    "            except:\n",
    "                try:\n",
    "                    currReviews = [json_data['flyfishSummaryResponse']['summary']['TA']['best'][0]['reviewText'],json_data['flyfishSummaryResponse']['summary']['TA']['best'][-1]['reviewText']]\n",
    "                except:\n",
    "                    try:\n",
    "                        currReviews = [json_data['flyfishSummaryResponse']['summary']['MANUAL']['best'][0]['reviewText'],json_data['flyfishSummaryResponse']['summary']['MANUAL']['best'][-1]['reviewText']]\n",
    "                    except:\n",
    "                        currReviews=[]\n",
    "        currData = {\n",
    "            \"HotelName\":currHotelName,\n",
    "            \"Country\":currCountry,\n",
    "            \"City\":data['cityName'],\n",
    "            \"StarRating\":currStarRating,\n",
    "            \"PricePerNight\":currPricePerNight,\n",
    "            \"Amenities\":currAmenities,\n",
    "            \"HotelDescription\":currHotelDescription,\n",
    "            \"Reviews\":currReviews\n",
    "        }\n",
    "        post_header['Referer']=\"\"\n",
    "        return currData\n",
    "    except Exception as e:\n",
    "        print('error : scrap_curr_hotel_page_filter')\n",
    "        print('error msg:',e)\n",
    "        return {\"failed\":True}\n"
   ]
